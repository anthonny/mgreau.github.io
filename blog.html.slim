---
layout: base
---
<div class="container">
    <h2>Blog</h2>
    <div class="row-fluid">
      <div class="span12 service"> 
            =partial('pagination.html.slim', :posts => page.posts)
	div style="clear: both"
	- page.posts.each do |post|
     - if post.date.strftime('%d/%m/%Y')!='27/09/2013'
	      =partial('entry.html.slim', :post => post, :listing => true)
	=partial('pagination.html.slim', :posts => page.posts)   
      </div>
    </div>
</div>

